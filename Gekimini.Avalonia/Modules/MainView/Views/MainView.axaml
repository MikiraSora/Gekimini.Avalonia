<views1:ViewBase xmlns="https://github.com/avaloniaui"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                 xmlns:viewModels="clr-namespace:Gekimini.Avalonia.Modules.MainView.ViewModels"
                 xmlns:views="clr-namespace:AvaloniaDialogs.Views;assembly=AvaloniaDialogs"
                 xmlns:views1="clr-namespace:Gekimini.Avalonia.Views"
                 mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
                 x:DataType="viewModels:MainViewModel"
                 x:Class="Gekimini.Avalonia.Modules.MainView.Views.MainView">
    <Design.DataContext>
        <viewModels:MainViewModel />
    </Design.DataContext>

    <views:ReactiveDialogHost OverlayBackground="WhiteSmoke">
        <Grid RowDefinitions="Auto,*">
            <Grid Height="20" Background="BurlyWood" />
            <Grid Grid.Row="1">
                <TransitioningContentControl Content="{Binding Shell}" />
            </Grid>
        </Grid>
    </views:ReactiveDialogHost>

</views1:ViewBase>